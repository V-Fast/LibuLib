{
  "schemaVersion": 1,
  "id": "libu",
  "version": "${version}",

  "name": "Libu",
  "description": "Library made for developers that doesn't like writing the same code over and over again, just like me, Lumaa.",
  "authors": [
    "LumaaDev"
  ],
  "contributors": [
    "PatateGivree"
  ],
  "contact": {
    "homepage": "https://lumaa.brebond.com/",
    "sources": "https://github.com/lumaa-dev/LibuLib",
    "issues": "https://github.com/lumaa-dev/LibuLib/issues"
  },

  "license": "MIT",
  "icon": "assets/libu/icon.png",

  "environment": "*",
  "entrypoints": {
